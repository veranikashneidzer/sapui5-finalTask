<mvc:View controllerName="project1.controller.Main"
    xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core"
    xmlns="sap.m">
    <Page id="page" title="Orders">
		    <List
				id="listOfOrders"
				width="auto"
				items="{DataV2>/Orders}">
				<items>
					<ObjectListItem title="{DataV2>OrderID}">
					</ObjectListItem>
				</items>
			</List>
    </Page>
</mvc:View>