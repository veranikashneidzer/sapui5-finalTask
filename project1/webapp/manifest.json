{
  "_version": "1.81.0",
  "sap.app": {
    "id": "project1",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.20.1",
      "toolsId": "4809c35b-1e11-4163-b8a5-2dcfca1e65bf"
    },
    "dataSources": {
      "ODataV2": {
        "uri": "/V2/OData/OData.svc/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0",
          "localUri": "localService/metadataV2.xml"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "handleValidation": true,
    "rootView": {
			"viewName": "project1.view.App",
			"type": "XML",
			"async": false,
			"id": "project1"
		},
    "dependencies": {
      "minUI5Version": "1.144.0",
      "libs": {
        "sap.f": {},
        "sap.m": {},
        "sap.ui.core": {},
        "sap.uxap": {},
        "sap.ui.layout": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "project1.i18n.i18n",
          "supportedLocales": [
            "en"
          ],
          "fallbackLocale": ""
        }
      },
      "DataV2": {
        "type": "sap.ui.model.odata.v2.ODataModel",
        "dataSource": "ODataV2",
        "settings": {
          "defaultBindingMode": "TwoWay"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
				"routerClass": "sap.f.routing.Router",
				"type": "View",
				"viewType": "XML",
				"path": "project1.view",
				"controlId": "layout",
				"controlAggregation": "beginColumnPages",
				"bypassed": {
					"target": "notFound"
				},
				"async": true
			},
      "routes": [
				{
					"pattern": "",
					"name": "RouteMain",
					"target": "TargetMain"
				},
				{
					"pattern": "Orders/{objectId}/:?query:",
					"name": "object",
					"target": [
						"TargetMain",
						"object"
					]
				}
			],
      "targets": {
        "TargetMain": {
					"name": "Main",
					"level": 1,
					"id": "Main"
				},
				"object": {
					"name": "Details",
					"id": "details",
					"level": 1,
					"controlAggregation": "midColumnPages"
				}
      }
    },
    "rootView": {
      "viewName": "project1.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}